    "enable_ml_predictor": true,
    "enable_alerts": true,
    "enable_integration": true,
    "api_port": 8002,
    "dashboard_port": 8003,
    "ml_model_storage_path": "ml_models",
    "alert_cooldown_minutes": 30,
    "health_check_interval": 60,
    "performance_monitoring": true,
